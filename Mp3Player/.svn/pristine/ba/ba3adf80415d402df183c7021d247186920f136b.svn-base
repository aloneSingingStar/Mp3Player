<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Black">
        <!-- Customize your theme here. -->
    </style>
    <!--自定义主题，parent继承父主题，item扩展父主题-->
    <style name="MyAppTheme" parent="android:Theme.Black">
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:panelColorBackground">#d70b17</item>-->
        <!--<item name="android:panelColorForeground">#FFFFFFFF</item>-->
    </style>
    <style name="launch_dialog">
        <item name="android:windowIsFloating">true</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <!--无标题-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--背景透明-->
        <item name="android:backgroundDimEnabled">true</item>
        <!--模糊-->
        <item name="android:backgroundDimAmount">0.5</item>
    </style>
<style name="rb_bottom_bar">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">fill_parent</item>
    <!--每个RadioButton平均分布-->
    <item name="android:layout_weight">1</item>
    <item name="android:textSize">12sp</item>
    <!--RadioButton 颜色选择 state_checked="true",如果Button被选中，显示红色，否则显示灰色-->
    <item name="android:textColor">@drawable/bottom_text_color_selector</item>
    <!--底部居中-->
    <item name="android:gravity">bottom|center</item>
    <!--不显示按钮-->
     <item name="android:button">@null</item>
    <item name="android:paddingTop">4dip</item>
    <item name="android:paddingBottom">2dip</item>
    <!--设置背景颜色，如果设置为空，则值为@null： <item name="android:background">@null</item>-->
    <item name="android:background">#FFD77BD7</item>
</style>

    <!-- 最外层布局 -->
    <style name="price_layout">
        <item name="android:background">@color/price_common_white</item>
    </style>

    <!-- 二级标题下的导航游标 -->
    <style name="price_second_level_title_nav_cursor">
        <item name="android:background">@color/price_bg_nav_transparent</item>
    </style>

    <!-- 沪深>下拉刷新界面 -->
    <style name="price_hushen_header_content">
        <item name="android:gravity">center</item>
    </style>
    <!-- 沪深>下拉刷新字体1 -->
    <style name="price_hushen_header_content_text1">
        <item name="android:textColor">@color/price_tc_hushen_header_content_gray</item>
        <item name="android:textSize">15sp</item>
    </style>
    <!-- 沪深>下拉刷新字体2 -->
    <style name="price_hushen_header_content_text2">
        <item name="android:textColor">@color/price_tc_hushen_header_content_gray</item>
        <item name="android:textSize">12sp</item>
    </style>
</resources>
